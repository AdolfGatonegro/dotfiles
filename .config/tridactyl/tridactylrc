sanitise tridactyllocal tridactylsync

" General Settings
set update.lastchecktime 1669487807901
set update.nag true
set update.nagwait 7
set update.lastnaggedversion 1.14.0
set update.checkintervalsecs 86400
set configversion 2.0
set newtab about:blank
set modeindicator false
set smoothscroll true
set editorcmd $TERMINAL -e $EDITOR %f
set hintfiltermode vimperator
set hintchars 1234567890
set allowautofocus false
set completions.Bmark.autoselect false
set completions.Tab.statusstylepretty true
set externalclipboardcmd xclip
colourscheme gatonegro

" Search Engines
"set searchurls.amus https://amazon.com/s?k=%s
set searchurls.aa https://annas-archive.org/search?q=%s&ext=epub
set searchurls.aw https://wiki.archlinux.org/index.php?search=%s
set searchurls.az https://amazon.com.mx/s?k=%s
set searchurls.d https://duckduckgo.com/?q=%s
set searchurls.g https://google.com/search?hl=en&&udm=14&q=%s
set searchurls.gb https://google.com/search?tbm=bks&q=%s
set searchurls.gh https://github.com/search?utf8=âœ“&q=%s
set searchurls.lg https://libgen.rs/search.php?req=%s&lg_topic=libgen&open=0&view=simple&res=25&phrase=1&column=def
